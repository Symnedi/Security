{
	"name": "symnedi/security",
	"description": "Symfony\\Security integration to Nette.",
	"keywords": ["nette", "symfony", "security"],
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz", "email": "tomas.vot@gmail.com" }
	],
	"require": {
		"php": ">=5.5",
		"kdyby/events": "^2.4",
		"nette/di": "^2.3",
		"nette/security": "^2.3",
		"symfony/security-core": "^2.6",
		"symfony/security-http": "^2.6"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"nette/bootstrap": "~2.3",
		"nette/robot-loader": "^2.3",
		"nette/application": "^2.3",
		"tracy/tracy": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"Symnedi\\Security\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symnedi\\Security\\Tests\\": "tests"
